<?xml version="1.0"?>
<robot name="origins">

<material name="Metall - 1600F (heiß)">
    <color rgba="1 0.561 0.184 1" />
  </material>

  <link name="base_link">
    <visual>
      <geometry>
        <cylinder length="0.6" radius="0.2"/>
      </geometry>
    </visual>
  </link>

  <link name="U10_Base">
    <visual>
     <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>  
            <mesh filename="../12345/Base.stl"/>
 
      </geometry> 
         <material name="Metall - 1600F (heiß)" />
    </visual>
  </link>

<link name="First_Gelenk">
    <visual>
     <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>  
            <mesh filename="../12345/UR10_Size4_Joint_meter.stl"/>
 
      </geometry> 
     
    </visual>
  </link>

<link name="Second_Gelenk">
    <visual>
     <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>  
            <mesh filename="../12345/UR10_Size4_Joint_meter.stl"/>
 
      </geometry> 
         <material name="Metall - 1600F (heiß)" />
    </visual>
  </link>

<link name="Upper_arm">
    <visual>
     <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>  
            <mesh filename="../12345/UR10_Upper_arm_meter.stl"/> 
      </geometry> 
    </visual>
  </link>

<link name="Fourth_Gelenk">
    <visual>
     <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>  
            <mesh filename="../12345/UR10_Size3Joint_meter.stl"/>
 
      </geometry> 
         <material name="Metall - 1600F (heiß)" />
    </visual>
  </link>

  <joint name="base_to_right_leg" type="fixed">
    <parent link="base_link"/>
    <child link="U10_Base"/>
    <origin xyz="0 1 0"/>
  </joint>

<joint name="first_joint" type="continuous">
         <axis rpy="0 0 0" xyz="0 0 1"/>
    <parent link="U10_Base"/>
    <child link="First_Gelenk"/>
    <origin xyz="0 0 0.128"/>
  </joint>
 
<joint name="second_joint" type="continuous">
         <axis rpy="0 0 0" xyz="0 1 0"/>
    <parent link="First_Gelenk"/>
    <child link="Second_Gelenk"/>
    <origin xyz="0 -0.176 0" rpy="1.571 3.1415 0"/>
  </joint>


<joint name="third_joint" type="fixed">
          
    <parent link="Second_Gelenk"/>
    <child link="Upper_arm"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>

<joint name="fourth_joint" type="continuous">
         <axis rpy="0 0 0" xyz="0 1 0"/>
    <parent link="Upper_arm"/>
    <child link="Fourth_Gelenk"/>
    <origin xyz="0 0 0.46" rpy="0 0 0"/>
  </joint>
</robot>
